<script type="text/javascript">
	function validasi(){
		var tgl_dari_picker = document.getElementsByName('bulan_awal')[0].value;
		var tgl_dari = tgl_dari_picker.split("/");
		var startDate = new Date();
		startDate.setDate(1);
		startDate.setMonth(tgl_dari[0]-1);
		startDate.setFullYear(tgl_dari[1]);

		var tgl_sampai_picker = document.getElementsByName('bulan_akhir')[0].value;
		var tgl_sampai = tgl_sampai_picker.split("/");
		var endDate = new Date();
		endDate.setDate(1);
		endDate.setMonth(tgl_sampai[0]-1);
		endDate.setFullYear(tgl_sampai[1]);
		// console.log('enddate', endDate);
		// return false;

		if(endDate < startDate){
			alert('tanggal sampai tidak valid');
			return false;
		}else{
			return true;
		}
	}
</script>

